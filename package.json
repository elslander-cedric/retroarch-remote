{
  "name": "retroarch-remote",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "concurrently \"npm run start-client\" \"npm run start-server\"",
    "start-server": "nodemon --watch 'src/server/' --ignore 'src/server/**/*.spec.ts' --exec ts-node -- src/server/Server.ts",
    "start-client": "ng serve --proxy-config proxy.conf.json",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "bundle": "npm run bundle-client && npm run bundle-server",
    "bundle-client": "ng build",
    "bundle-server": "webpack --config webpack.server-config.js"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.4.9",
    "@angular/compiler": "^2.4.9",
    "@angular/core": "^2.4.9",
    "@angular/flex-layout": "^2.0.0-rc.1",
    "@angular/forms": "^2.4.9",
    "@angular/http": "^2.4.9",
    "@angular/material": "^2.0.0-beta.2",
    "@angular/platform-browser": "^2.4.9",
    "@angular/platform-browser-dynamic": "^2.4.9",
    "@angular/router": "^3.4.9",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.18",
    "bluebird": "^3.4.7",
    "bootstrap": "^4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "md2": "0.0.16",
    "request": "^2.79.0",
    "rxjs": "^5.2.0",
    "sync-request": "^4.0.1",
    "ts-helpers": "^1.1.1",
    "unzip": "^0.1.11",
    "zone.js": "^0.7.7"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.4.9",
    "@angular/cli": "1.0.0-beta.31",
    "@types/jasmine": "^2.5.43",
    "@types/node": "^7.0.5",
    "codelyzer": "^3.0.0-beta.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.6.0",
    "protractor": "~5.1.1",
    "ts-loader": "^2.0.1",
    "ts-node": "^2.1.0",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1",
    "webpack": "^2.2.0"
  }
}
