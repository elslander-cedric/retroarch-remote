{
  "manifest_version": 2,
  "name": "RetroArch Remote",
  "version": "1.0.0",
  "default_locale": "en",
  "description": "Remote control RetroArch",
  "background_color": "#fff",
  "start_url": "index.html",
  "display": "standalone",
  "orientation": "portrait",
  "icons": [
    {
      "src": "/assets/img/retroarch-remote-48x48.png",
      "sizes": "48x48",
      "type": "image/png",
      "density": 1.0
    },
    {
      "src": "/assets/img/retroarch-remote-72x72.png",
      "sizes": "72x72",
      "type": "image/png",
      "density": 1.5
    },
    {
      "src": "/assets/img/retroarch-remote-96x96.png",
      "sizes": "96x96",
      "type": "image/png",
      "density": 2.0
    },
    {
      "src": "/assets/img/retroarch-remote-144x144.png",
      "sizes": "144x144",
      "type": "image/png",
      "density": 3.0
    },
    {
      "src": "/assets/img/retroarch-remote-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "density": 4.0
    }
  ]
}
